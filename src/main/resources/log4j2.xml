<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{mm:ss.SSS} %-5level [%t] - %msg%n" />
        </Console>
    </Appenders>

    <Loggers>
        <Root level="TRACE" additivity="false">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>
</Configuration>
